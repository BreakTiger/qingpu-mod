.container {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}

.top-status-line {
  width: 100%;
  height: 100rpx;
  background-image: linear-gradient(-45deg, #69b0ff 0%, #2c6cb4 100%);
  display: flex;
  flex-direction: row;
  align-items: center;
}

.top-status-line .icon {
  width: 40rpx;
  height: 40rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
}

.top-status-line text {
  font-size: 42rpx;
  color: #fff;
}

.address-box {
  width: 100%;
  height: 240rpx;
}

.address-info {
  width: 100%;
  height: 180rpx;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  flex-direction: row;
}

.adrs {
  width: 40rpx;
  height: 50rpx;
  margin-top: 45rpx;
}

.info-box {
  width: 560rpx;
  height: 150rpx;
}

.info {
  font-size: 28rpx;
  color: #343434;
  margin-bottom: 20rpx;
}

.reala {
  font-size: 28rpx;
}

.feng {
  width: 100%;
  height: 8rpx;
  display: block;
}

.goods {
  width: 100%;
  height: auto;
  background: #f5f4f5;
}

.goodsbox {
  width: 100%;
  height: 250rpx;
  box-sizing: border-box;
  padding: 20rpx 30rpx;
  display: flex;
  justify-content: space-between;
  flex-direction: row;
}

.thumb {
  width: 200rpx;
  height: 200rpx;
  border-radius: 8rpx;
}

.goodsinfo {
  width: 500rpx;
  font-size: 28rpx;
  box-sizing: border-box;
  padding-left: 20rpx;
}

.gdmsg {
  width: 100%;
  height: 80rpx;
  color: #343434;
  font-size: 28rpx;
}

.color {
  font-size: 26rpx;
  color: #acacac;
}

.size {
  margin-top: 10rpx;
  font-size: 26rpx;
  color: #acacac;
}

.price {
  margin-top: 10rpx;
  color: #ff6700;
  display: flex;
  justify-content: space-between;
  flex-direction: row;
}

.print {
  width: 100%;
  height: 180rpx;
  color: #343434;
  font-size: 28rpx;
  line-height: 180rpx;
  box-sizing: border-box;
  padding: 0 30rpx;
  border-bottom: 1rpx solid #e4e4e4;
  display: flex;
  justify-content: space-between;
  flex-direction: row;
}

.print-thumb {
  width: 170rpx;
  height: 130rpx;
  margin-top: 25rpx;
}

.oderxj {
  width: 100%;
  height: 80rpx;
  color: #343434;
  font-size: 28rpx;
  line-height: 80rpx;
  box-sizing: border-box;
  padding: 0 30rpx;
  border-bottom: 1rpx solid #e4e4e4;
  display: flex;
  justify-content: space-between;
  flex-direction: row;
}

.ordernum {
  font-size: 28rpx;
  color: #9b9c9b;
  padding: 8rpx 30rpx;
}

