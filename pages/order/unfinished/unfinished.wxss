.container{
  width: 100%;
  height: auto;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.icon{
  width: 243rpx;
  height: 193rpx;
  margin-bottom: 70rpx;
  margin-top: 230rpx;
}

.hint{
  font-size:36rpx;
  color: #333; 
  margin-bottom: 40rpx;
}

.infobox{
  width: 670rpx;
  height: 240rpx;
  background-color: #f5f5f5;
}

.infobox .title{
  font-size: 30rpx;
  margin-left: 25rpx;
  margin-top: 25rpx;
}

.address-box{
  width: 670rpx;
  height: auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 30rpx;
}

.address-box text{
  font-size: 30rpx;
  color: #ff4949;
  line-height: 45rpx;
}